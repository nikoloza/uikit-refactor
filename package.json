{
  "name": "@rackai/symbols",
  "description": "UI Library built on Scratch and DOMQL",
  "private": false,
  "author": "rackai",
  "version": "0.0.6",
  "repository": "https://github.com/rackai/symbols",
  "main": "src/index.js",
  "scripts": {
    "start": "parcel showcase/index.html",
    "build": "parcel build showcase/index.html --public-url .",
    "deploy": "yarn build && gh-pages -d dist",
    "reinstall": "rm yarn.lock && rm -rf node_modules/domql node_modules/scratch && yarn"
  },
  "dependencies": {
    "@rackai/domql": "0.0.5",
    "@rackai/scratch": "0.0.1"
  },
  "alias": {
    "domql": "~/node_modules/@rackai/domql",
    "scratch": "~/node_modules/@rackai/scratch",
    "^": "~/src"
  },
  "devDependencies": {
    "emotion": "^10.0.27",
    "gh-pages": "^2.2.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-svg-sprite": "^1.4.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
